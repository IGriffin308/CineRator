{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/userAuth.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "is_admin": {
			"type": "boolean",
			"default": false
		}
  },
  "additionalProperties": false,
  "required": [
    "username",
    "password",
    "is_admin"
  ]
}